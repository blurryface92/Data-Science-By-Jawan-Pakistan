    "#Assignment 02\n",
    "\n",
    "import pandas as pd\n",
    "\n",
    "data = pd.read_csv('Pokemon.csv')\n",
    "\n",
    "#add high_low_speed column\n",
    "data['high_low_speed'] = data['Speed'].mean()\n",
    "\n",
    "\n",
    "#add high_low_hp column\n",
    "data['high_low_hp'] = data['HP'].mean()\n",
    "print(data.head())\n",
    "\n",
    "print(data.describe())\n"
